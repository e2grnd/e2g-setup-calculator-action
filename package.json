{
  "name": "@e2grnd/e2g-setup-calculator-action",
  "version": "0.1.0",
  "private": true,
  "description": "GitHub Action to emit configuration for internal use.",
  "main": "lib/main.js",
  "scripts": {
    "clean": "git clean -xfd lib dist",
    "build": "tsc",
    "write-status": "node lib/write-status-badges.js",
    "write-clone-script": "node lib/write-clone-script.js",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "yarn lint && yarn clean && yarn build && yarn write-status && yarn write-clone-script && yarn format && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2grnd/e2g-setup-calculator-action.git"
  },
  "author": "alampros@e2g.com",
  "license": "UNLICENSED",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^5.0.3",
    "lodash.kebabcase": "^4.1.1"
  },
  "devDependencies": {
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/node": "^16.11.59",
    "@typescript-eslint/parser": "^5.37.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-prettier": "^4.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "typescript": "^4.8.3"
  },
  "packageManager": "yarn@3.2.3"
}
